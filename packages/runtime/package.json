{
  "name": "@ficusjs/app-shell-runtime",
  "version": "0.1.0",
  "description": "Ficus App Shell Runtime",
  "type": "module",
  "main": "src/main.mjs",
  "module": "src/main.mjs",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "run-p clean build:*",
    "build:main": "rollup -i src/main.mjs -d dist -f es -m --preserveEntrySignatures=strict -p \"terser={mangle: { keep_fnames: true, toplevel: true, module: true }}\" -p \"filesize={}\" -p \"node-resolve={}\"",
    "clean": "rimraf dist tmp",
    "lint": "standard src/**/*.mjs",
    "publish": "",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ficusjs/ficusjs-app-shell.git"
  },
  "keywords": [
    "ficusjs",
    "frontend",
    "web-components",
    "app-shell"
  ],
  "author": "Matt Levy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ficusjs/ficusjs-app-shell/issues"
  },
  "standard": {
    "ignore": [
      "*.ts"
    ]
  },
  "homepage": "https://github.com/ficusjs/ficusjs-app-shell#readme",
  "dependencies": {
    "@ficusjs/core": "2.1.0",
    "@ficusjs/event-bus": "1.3.0",
    "@ficusjs/i18n": "1.1.0",
    "@ficusjs/renderers": "5.4.1",
    "@ficusjs/router": "3.4.0",
    "@ficusjs/state": "3.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.0.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-terser": "7.0.2",
    "standard": "16.0.4"
  },
  "exports": {
    ".": {
      "import": "./src/main.mjs"
    }
  }
}
