{
  "name": "@ficusjs/app-shell-ui",
  "version": "0.1.0",
  "description": "UI components for the FicusJS App Shell",
  "type": "module",
  "main": "/src/main.mjs",
  "module": "/src/main.mjs",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "run-p clean build:js",
    "build:js": "rollup -i src/main.mjs -o dist/main.mjs -f es -m -p \"terser={mangle: { keep_fnames: true, toplevel: true, module: true }}\" -p \"filesize={}\" -p \"minify-html-literals={}\" -p \"node-resolve={}\"",
    "clean": "rimraf dist tmp",
    "publish": "",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ficusjs/ficusjs-app-shell.git"
  },
  "keywords": [
    "ficusjs",
    "frontend",
    "web-components",
    "app-shell"
  ],
  "author": "Matt Levy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ficusjs/ficusjs-app-shell/issues"
  },
  "standard": {
    "ignore": [
      "*.ts"
    ]
  },
  "homepage": "https://github.com/ficusjs/ficusjs-app-shell#readme",
  "devDependencies": {
    "@ficusjs/resource-loader": "^0.1.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "rollup-plugin-terser": "7.0.2",
    "standard": "16.0.4"
  },
  "exports": {
    ".": {
      "import": "./src/main.mjs"
    }
  }
}
